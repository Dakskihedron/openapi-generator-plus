  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.28",
    "@types/pluralize": "0.0.29",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
