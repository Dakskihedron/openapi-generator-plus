  "scripts": {
    "build": "lerna run build",
    "clean": "lerna run clean",
    "link": "lerna exec npm link && lerna clean --yes && rimraf packages/*/package-lock.json",
    "postinstall": "patch-package",
    "update": "npx lerna-update-wizard",
